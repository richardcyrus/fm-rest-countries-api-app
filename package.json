    "axe-core": "^4.11.0",
    "cypress": "^10.11.0",
    "cypress-axe": "^1.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-config-react-app": "^7.0.1",
